{
    "name": "imiphp/annotation-migration",
    "type": "project",
    "license": "MulanPSL-2.0",
    "description": "工具：迁移 imi 注解为 PHP8 原生实现",
    "autoload": {
        "psr-4": {
            "Imiphp\\Tool\\AnnotationMigration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imiphp\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "nikic/php-parser": "^4.17.1",
        "symfony/finder": "^5.4|^6.0",
        "symfony/console": "^5.4|^6.0",
        "psr/log": "~2.0|~3.0",
        "yurunsoft/doctrine-annotations": "^1.104.3"
    },
    "require-dev": {
        "imiphp/imi": "3.0.x-dev#36c5270",
        "php-cs-fixer/shim": "~3.35.1",
        "phpunit/phpunit": "^10.4",
        "symfony/var-dumper": "^6.3"
    },
    "bin": [
        "bin/imi-migration"
    ],
    "config": {
        "sort-packages": true
    }
}
